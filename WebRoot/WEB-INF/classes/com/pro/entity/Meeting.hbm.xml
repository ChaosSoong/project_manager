<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_meeting" name="com.pro.entity.Meeting">
    <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
    </id>
    <property name="title" access="field" length="140" type="java.lang.String"/>
    <property name="remark" access="field" length="1400" type="java.lang.String"/>
  	<property name="postDate" access="field" length="80" type="java.lang.String"/>
  	<property name="beginDate" access="field" length="80" type="java.lang.String"/>
	<property name="beginHour" access="field" length="12" type="java.lang.String"/>
	<property name="beginMiniute" access="field" length="12" type="java.lang.String"/>
	<property name="postUser" access="field" length="80" type="java.lang.String"/>
	<property name="isPost" access="field" length="8" type="java.lang.String"/>
	<property name="address" access="field" length="188" type="java.lang.String"/>
  </class>
</hibernate-mapping>